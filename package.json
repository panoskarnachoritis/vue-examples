{
  "name": "vue-examples",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite --port 80",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "start": "concurrently \"vite --port 80\" \"npx json-server db.json -p 3001\""
  },
  "dependencies": {
    "vue": "^3.2.47",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-prettier": "^7.1.0",
    "eslint": "^8.34.0",
    "eslint-plugin-vue": "^9.9.0",
    "json-server": "^0.17.3",
    "prettier": "^2.8.4",
    "vite": "^4.1.4",
    "concurrently": "^8.0.1",
    "npx-server": "^0.0.47"
  }
}
